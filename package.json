{
  "name": "yan-run",
  "version": "1.0.0",
  "description": "Yandex Praktikum Module 2 Project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "check:code": "tsc --noEmit",
    "lint:code": "eslint ./src --ext .ts,.tsx",
    "lint:codefix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:css": "stylelint src/client/css/*.scss",
    "lint:cssfix": "stylelint src/client/css/*.scss --fix",
    "clean": "rimraf dist",
    "build:api": "webpack --config webpack/api-server.config.ts",
    "build:web:client": "webpack --config webpack/client.config.ts",
    "build:web:server": "webpack --config webpack/server.config.ts",
    "build:web": "npm-run-all --print-label build:web:*",
    "build:all": "webpack",
    "build": "npm-run-all clean build:all",
    "server:web": "node index.js",
    "start:web": "npm-run-all --print-label build:web:server server:web",
    "watch:api": "webpack --config webpack/api-server.config.ts --watch",
    "server:watch:api": "nodemon api-index.js --watch dist/api-server.js",
    "start:api": "npm-run-all --parallel --print-label watch:api server:watch:api",
    "start": "npm-run-all --parallel --print-label clean start:web start:api",
    "docker:api": "docker build --platform linux/amd64 -t dubaidev/yanrun-api:latest -f ./src/api-server/Dockerfile .",
    "docker:web": "docker build --platform linux/amd64 -t dubaidev/yanrun-web:latest -f ./src/client/Dockerfile ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanprak/yan-run.git"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanprak/yan-run/issues"
  },
  "homepage": "https://github.com/yanprak/yan-run#readme",
