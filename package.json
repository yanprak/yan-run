{
  "name": "yan-run",
  "version": "1.0.0",
  "description": "Yandex Praktikum Module 2 Project",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "check:code": "tsc --noEmit",
    "lint:code": "eslint ./src --ext .ts,.tsx",
    "lint:codefix": "eslint ./src --ext .ts,.tsx --fix",
    "lint:css": "stylelint src/client/css/*.scss",
    "lint:cssfix": "stylelint src/client/css/*.scss --fix",

    "clean": "rimraf dist",

    "build:dev-ssr-server": "cross-env NODE_ENV=development SR=server webpack --config webpack/server.config.ts",
    "build:dev-csr-server": "cross-env NODE_ENV=development SR=client webpack --config webpack/server.config.ts",

    "watch:ssr-client": "cross-env NODE_ENV=development SR=server webpack --watch --config webpack/client.config.ts",
    "watch:csr-client": "cross-env NODE_ENV=development SR=client webpack --watch --config webpack/client.config.ts",

    "server:dev-ssr": "cross-env NODE_ENV=development SR=server node index.js",
    "server:dev-csr": "cross-env NODE_ENV=development SR=client node index.js",

    "server:ssr": "cross-env NODE_ENV=production SR=server node index.js",
    "server:csr": "cross-env NODE_ENV=production SR=client node index.js",

    "start:ssr-watch": "npm-run-all --print-label --parallel server:dev-ssr watch:ssr-client",
    "start:csr-watch": "npm-run-all --print-label --parallel server:dev-csr watch:csr-client",

    "start:ssr": "npm-run-all clean build:dev-ssr-server start:ssr-watch --print-label",
    "start:csr": "npm-run-all clean build:dev-csr-server start:csr-watch --print-label",

    "build:ssr": "cross-env NODE_ENV=production SR=server npm run clean && webpack",
    "build:csr": "cross-env NODE_ENV=production SR=client npm run clean && webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanprak/yan-run.git"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanprak/yan-run/issues"
  },
  "homepage": "https://github.com/yanprak/yan-run#readme",
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "7.13.8",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@testing-library/react": "11.2.5",
    "@types/classnames": "2.2.11",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-hot-loader": "4.1.1",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/react-router-redux": "5.0.18",
    "@types/react-test-renderer": "17.0.1",
    "@types/redux": "3.6.0",
    "@types/serialize-javascript": "5.0.0",
    "@types/webpack-dev-middleware": "4.1.0",
    "@types/webpack-hot-middleware": "2.25.3",
    "@types/webpack-node-externals": "2.5.0",
    "@typescript-eslint/eslint-plugin": "4.14.2",
